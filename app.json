{
  "name": "enebular-agent-heroku",
  "description": "The enebular oneoff app is used to Data Management. For more about enebular see enebular.com.",
  "keywords": [
    "enebular",
    "IoT",
    "Internet of Things",
    "node.js",
    "node"
  ],
  "website": "https://enebular.com/",
  "repository": "https://github.com/Uhuru-Corporation/enebular-oneoff-app.git",
  "logo": "https://avatars1.githubusercontent.com/u/8124914",
  "env": {
    "USERNAME": {
      "description": "username",
      "value": "admin"
    },
    "PASSWORD": {
      "description": "password",
      "value": "password"
    },
    "AGENT_HEROKU_VERSION": {
      "description": "versioin of enebular agent heroku (do not change)",
      "value": "1.4.0"
    }
  },
  "addons": [
    {
      "plan": "heroku-postgresql:mini",
      "options": {
        "version": "14"
      }
    }
  ]
}